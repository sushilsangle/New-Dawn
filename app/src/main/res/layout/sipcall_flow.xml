        android:textColor="@android:color/holo_green_dark"
        android:layout_margin="5dp"
        android:gravity="center"
        android:layout_below="@id/img_sip"
        android:text="--------------------------------------------------------------------------------------"/>


    <android.support.v7.widget.AppCompatTextView
        android:id="@+id/txt_sip_mean"
       style="@style/SIPFlowCMD"
        android:layout_below="@+id/txt_dot_sip"
        android:text="     What is mean by SIP.?"
       />
    <android.support.v7.widget.AppCompatTextView
        style="@style/SIPCallFlowArrow"
        android:layout_below="@+id/txt_sip_mean"
        android:text="->" />
   <android.support.v7.widget.AppCompatTextView
    android:id="@+id/txt_firstan"
    style="@style/SIPCallFlowText"
    android:layout_below="@+id/txt_sip_mean"
    android:text="SIP stands for Session Initiation Protocol."/>

<android.support.v7.widget.AppCompatTextView
   style="@style/SIPCallFlowArrow"
    android:layout_below="@+id/txt_firstan"
    android:text="->"/>
    <android.support.v7.widget.AppCompatTextView
        android:id="@+id/txt_sip_call_s"
        style="@style/SIPCallFlowText"
        android:layout_below="@+id/txt_firstan"
        android:text="It used to establish, modify , terminate multimedia sessions over the Internet Protocol."/>

    <android.support.v7.widget.AppCompatTextView
        style="@style/SIPCallFlowArrow"
        android:layout_below="@+id/txt_sip_call_s"
        android:id="@+id/txt_sipt"
        android:text="->"/>
    <android.support.v7.widget.AppCompatTextView
       style="@style/SIPCallFlowText"
        android:id="@+id/txt_sipf"
        android:layout_below="@+id/txt_sip_call_s"
        android:text="A session is nothing but a simple call between two endpoints.End point can be laptop,phone or any device."/>
    <android.support.v7.widget.AppCompatTextView
        style="@style/SIPCallFlowArrow"
        android:id="@+id/txt_sip_ei"
        android:layout_below="@+id/txt_sipf"
        android:text="->"/>
