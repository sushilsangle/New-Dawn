    <android.support.v7.widget.AppCompatTextView
        style="@style/SIPFlowCMD"
        android:id="@+id/txt_sip_optionn"
        android:layout_below="@+id/txt_sip_cance"
        android:text="       OPTIONS"/>
    <android.support.v7.widget.AppCompatTextView
        style="@style/SIPCallFlowArrow"
        android:id="@+id/txt_sip_option"
        android:layout_below="@+id/txt_sip_optionn"
        android:text="->"/>
    <android.support.v7.widget.AppCompatTextView
        style="@style/SIPCallFlowText"
        android:id="@+id/txt_sip_options"
        android:layout_marginStart="25dp"
        android:layout_below="@+id/txt_sip_optionn"
        android:text="OPTIONS is used to query a user agent or a proxy server about its capabilities and discover its current availability.The proxy never generates an OPTIONS request."/>


    <android.support.v7.widget.AppCompatTextView
