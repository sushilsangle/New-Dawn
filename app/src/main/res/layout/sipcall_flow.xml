        android:id="@+id/txt_sipei"
        android:layout_below="@+id/txt_sipf"
        android:text="SIP is an open source and application layer protocol defined by IETF (Internet Engineering Task Force) standard. It is defined in RFC 3261."/>


    <android.support.v7.widget.AppCompatTextView
       style="@style/SIPCallFlowArrow"
        android:id="@+id/txt_sipten"
        android:layout_below="@+id/txt_sipei"
        android:text="->"/>

    <android.support.v7.widget.AppCompatTextView
       style="@style/SIPCallFlowText"
        android:id="@+id/txt_sipele"
        android:layout_below="@+id/txt_sipei"
        android:text="It is easy to develop,implement and troubleshoot application layer. It is a HTTP,Web protocol or SMTP. "/>

    <android.support.v7.widget.AppCompatTextView
        style="@style/SIPCallFlowArrow"
        android:id="@+id/txt_tl"
        android:layout_below="@+id/txt_sipele"
        android:text="->"/>

    <android.support.v7.widget.AppCompatTextView
        style="@style/SIPCallFlowText"
        android:id="@+id/txt_thir"
        android:layout_below="@+id/txt_sipele"
        android:text="It consist Messages of headers and message body uses like client-server model"/>


    <android.support.v7.widget.AppCompatTextView
        style="@style/SIPCallFlowArrow"
        android:id="@+id/txt_four"
        android:layout_below="@+id/txt_thir"
        android:text="->"/>
    <android.support.v7.widget.AppCompatTextView
        style="@style/SIPCallFlowText"
        android:id="@+id/txt_fourte"
        android:layout_below="@+id/txt_thir"
        android:text="It is text based protocol which used UTF-8 encoding and alternate to H.323"/>



    <android.support.v7.widget.AppCompatTextView
        style="@style/SIPCallFlowArrow"
        android:id="@+id/txt_sdp_arro"
        android:layout_below="@+id/txt_fourte"
        android:text="->"/>
    <android.support.v7.widget.AppCompatTextView
        style="@style/SIPCallFlowText"
        android:id="@+id/txt_sip_sdp"
        android:layout_below="@+id/txt_fourte"
        android:text="SIP takes help from SDP (Session Description Protocol)which describessession and RTP (Real Time Transport Protocol) used for delivering voice and video over IP network."/>

    <android.support.v7.widget.AppCompatTextView
        style="@style/SIPCallFlowArrow"
