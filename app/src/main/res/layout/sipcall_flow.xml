    <android.support.v7.widget.AppCompatTextView
        style="@style/SIPCallFlowText"
        android:id="@+id/txt_bere"
        android:textColor="@android:color/holo_green_dark"
        android:layout_below="@+id/txt_sip_sig"
        android:text="Bearer (Media):- "/>
    <android.support.v7.widget.AppCompatTextView
        style="@style/SIPCallFlowText"
        android:id="@+id/txt_sip_berer"
        android:layout_marginStart="129dp"
