    <ImageView
        android:id="@+id/img_phonr"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:layout_marginStart="315dp"
        android:layout_below="@+id/txt_sip_berer"
        android:background="@drawable/img_phoner"/>


    <android.support.v7.widget.AppCompatTextView
       style="@style/SIPFlowCMD"
        android:id="@+id/txt_sip_rfc"
        android:layout_below="@+id/img_phonr"
        android:text="     What is mean by RFC.?"/>
    <android.support.v7.widget.AppCompatTextView
       style="@style/SIPCallFlowArrow"
        android:id="@+id/txt_sip_si"
        android:layout_below="@+id/txt_sip_rfc"
        android:text="->"/>

    <android.support.v7.widget.AppCompatTextView
        style="@style/SIPCallFlowText"
        android:id="@+id/txt_sip_six"
        android:layout_below="@+id/txt_sip_rfc"
        android:text="RFC stands for Request for Comments. It defines the most current  SIP version like 2.0 also it Deprecates RFC 2543."/>


    <android.support.v7.widget.AppCompatTextView
        style="@style/SIPFlowCMD"
        android:id="@+id/txt_sip_protocol"
        android:layout_below="@+id/txt_sip_six"
        android:text="     What are the protocols used in SIP.?"/>
    <android.support.v7.widget.AppCompatTextView
        style="@style/SIPCallFlowArrow"
        android:id="@+id/txt_prot"
        android:layout_below="@+id/txt_sip_protocol"
        android:text="->"/>
    <android.support.v7.widget.AppCompatTextView
        style="@style/SIPCallFlowText"
        android:id="@+id/txt_sip_proto"
        android:layout_below="@+id/txt_sip_protocol"
        android:text="1. Real-time transport protocol (RTP)"/>

    <android.support.v7.widget.AppCompatTextView
        style="@style/SIPCallFlowArrow"
        android:id="@+id/txt_protu"
        android:layout_below="@+id/txt_sip_proto"
        android:text="->"/>
    <android.support.v7.widget.AppCompatTextView
        style="@style/SIPCallFlowText"
        android:id="@+id/txt_sip_protot"
        android:layout_below="@+id/txt_sip_proto"
        android:text="2. Session Description Protocol (SDP)"/>
<android.support.v7.widget.AppCompatTextView
    style="@style/SIPFlowCMD"
    android:id="@+id/txt_sip_entity"
    android:layout_below="@+id/txt_sip_protot"
    android:text="     There are some entities which helps to creating SIP network."/>

    <android.support.v7.widget.AppCompatTextView
       style="@style/SIPCallFlowArrow"
        android:id="@+id/txt_sip_ent"
        android:layout_below="@+id/txt_sip_entity"
        android:text="-> UA"/>
    <android.support.v7.widget.AppCompatTextView
       style="@style/SIPCallFlowText"
        android:id="@+id/txt_sip_enty"
        android:layout_below="@+id/txt_sip_entity"
        android:text="      stands for User Agent. It is IP endpoint session and most important SIP element. It works as both user agent client and server. "/>


    <android.support.v7.widget.AppCompatTextView
        style="@style/SIPCallFlowArrow"
        android:id="@+id/txt_sip_ua"
        android:layout_below="@+id/txt_sip_enty"
        android:text="-> UAC"/>
    <android.support.v7.widget.AppCompatTextView
        style="@style/SIPCallFlowText"
        android:id="@+id/txt_sip_uac"
        android:layout_below="@+id/txt_sip_enty"
        android:text="         stands for User Agent client.It is logical entity which creates new SIP request. It sends request and receives response from client side."/>


    <android.support.v7.widget.AppCompatTextView
        style="@style/SIPCallFlowArrow"
        android:id="@+id/txt_sip_uass"
        android:layout_below="@+id/txt_sip_uac"
        android:text="-> UAS"/>
    <android.support.v7.widget.AppCompatTextView
        style="@style/SIPCallFlowText"
        android:id="@+id/txt_sip_uas"
        android:layout_below="@+id/txt_sip_uac"
        android:text="        stands for User Agent Server.It is logical entity which genrate request. It receives request and sends response."/>


    <android.support.v7.widget.AppCompatTextView
        style="@style/SIPFlowCMD"
        android:id="@+id/txt_sip_request"
        android:layout_below="@+id/txt_sip_uas"
        android:text="     Following is the SIP call flow"/>


    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/im_sip_call_flow"
        android:layout_margin="5dp"
        android:layout_below="@+id/txt_sip_request"
        android:background="@drawable/callflow"/>

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/im_sip_trace"
        android:layout_margin="5dp"
        android:layout_below="@+id/im_sip_call_flow"
        android:background="@drawable/sip_trace"/>



    <android.support.v7.widget.AppCompatTextView
        style="@style/SIPFlowCMD"
        android:id="@+id/txt_sip_rec"
        android:layout_below="@+id/im_sip_trace"
        android:text="       INVITE"/>

    <android.support.v7.widget.AppCompatTextView
       style="@style/SIPCallFlowArrow"
        android:id="@+id/txt_sip_re"
        android:layout_below="@+id/txt_sip_rec"
        android:text="->"/>
    <android.support.v7.widget.AppCompatTextView
       style="@style/SIPCallFlowText"
        android:id="@+id/txt_sip_invite"
        android:layout_below="@+id/txt_sip_rec"
        android:layout_marginStart="25dp"
        android:text="INVITE can contain media information of caller in message body.A successful INVITE request establishes dialog between two user agents which continues until BYE is sent to terminate the session."/>

    <android.support.v7.widget.AppCompatTextView
        style="@style/SIPFlowCMD"
        android:id="@+id/txt_sip_acc"
        android:layout_below="@+id/txt_sip_invite"
        android:text="       ACK"/>
    <android.support.v7.widget.AppCompatTextView
        style="@style/SIPCallFlowArrow"
        android:id="@+id/txt_sip_ac"
        android:layout_below="@+id/txt_sip_acc"
        android:text="->"/>
    <android.support.v7.widget.AppCompatTextView
        style="@style/SIPCallFlowText"
        android:id="@+id/txt_sip_ack"
        android:layout_marginStart="25dp"
        android:layout_below="@+id/txt_sip_acc"
        android:text="ACK confirms that client has received a final response to an INVITE message"/>


    <android.support.v7.widget.AppCompatTextView
        style="@style/SIPCallFlowArrow"
        android:id="@+id/txt_sip_acks"
        android:layout_below="@+id/txt_sip_ack"
        android:text="->"/>

    <android.support.v7.widget.AppCompatTextView
        style="@style/SIPCallFlowText"
        android:id="@+id/txt_sip_acku"
        android:layout_marginStart="25dp"
        android:layout_below="@+id/txt_sip_ack"
        android:text="ACK may not be used to modify the media description that has already been sent in the initial INVITE."/>

    <android.support.v7.widget.AppCompatTextView
        style="@style/SIPFlowCMD"
        android:id="@+id/txt_sip_byy"
        android:layout_below="@+id/txt_sip_acku"
        android:text="       BYE"/>
    <android.support.v7.widget.AppCompatTextView
        style="@style/SIPCallFlowArrow"
        android:id="@+id/txt_sip_by"
        android:layout_below="@+id/txt_sip_byy"
        android:text="->"/>
    <android.support.v7.widget.AppCompatTextView
        style="@style/SIPCallFlowText"
        android:id="@+id/txt_sip_bye"
        android:layout_marginStart="25dp"
        android:layout_below="@+id/txt_sip_byy"
        android:text="This is a SIP request that can be sent by either the caller or the called to end a session or Terminates the call."/>

    <android.support.v7.widget.AppCompatTextView
        style="@style/SIPFlowCMD"
        android:id="@+id/txt_sip_cancell"
        android:layout_below="@+id/txt_sip_bye"
        android:text="       CANCEL"/>
    <android.support.v7.widget.AppCompatTextView
        style="@style/SIPCallFlowArrow"
        android:id="@+id/txt_sip_cancel"
        android:layout_below="@+id/txt_sip_cancell"
        android:text="->"/>
    <android.support.v7.widget.AppCompatTextView
        style="@style/SIPCallFlowText"
        android:id="@+id/txt_sip_cance"
        android:layout_marginStart="25dp"
        android:layout_below="@+id/txt_sip_cancell"
