    <android.support.v7.widget.AppCompatTextView
        style="@style/SIPCallFlowArrow"
        android:id="@+id/txt_sip_regist"
        android:layout_below="@+id/txt_sip_register"
        android:text="->"/>
    <android.support.v7.widget.AppCompatTextView
        style="@style/SIPCallFlowText"
        android:id="@+id/txt_sip_registu"
        android:layout_marginStart="25dp"
        android:layout_marginBottom="20dp"
        android:layout_below="@+id/txt_sip_register"
