        android:layout_below="@+id/txt_sip_sdp"
        android:text="->"/>
    <android.support.v7.widget.AppCompatTextView
        style="@style/SIPCallFlowText"
        android:id="@+id/txt_sip_unicst"
        android:layout_below="@+id/txt_sip_sdp"
        android:text="SIP can be used for two-party unicast or multiparty multicast sessions.."/>
    <android.support.v7.widget.AppCompatTextView
        style="@style/SIPCallFlowArrow"
        android:id="@+id/txt_sdp_tcp"
        android:layout_below="@+id/txt_sip_unicst"
        android:text="->"/>
    <android.support.v7.widget.AppCompatTextView
        style="@style/SIPCallFlowText"
        android:id="@+id/txt_sip_tcp"
        android:layout_below="@+id/txt_sip_unicst"
        android:text="SIP applications can run on TCP, UDP protocols."/>


    <android.support.v7.widget.AppCompatTextView
        style="@style/SIPCallFlowArrow"
        android:id="@+id/txt_sdp_prot"
        android:layout_below="@+id/txt_sip_tcp"
        android:text="->"/>
    <android.support.v7.widget.AppCompatTextView
        style="@style/SIPCallFlowText"
        android:id="@+id/txt_sip_prot"
        android:layout_below="@+id/txt_sip_tcp"
        android:text="@string/sip_can_supports_following_protocols_1_sdp_for_media_handling_2_rtp_for_media_transport"/>


    <!--TODO write above text id to below text  -->

    <android.support.v7.widget.AppCompatTextView
       style="@style/SIPFlowCMD"
        android:id="@+id/txt_sip_fif"
        android:layout_below="@+id/txt_sip_prot"
        android:text="     What are the SIP componant.?"/>
    <android.support.v7.widget.AppCompatTextView
